<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Google Trends</title>
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
      integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <header id="main-header" class="bg-success p-4 mb-4 text-white">
      <div class="container">
        <div class="row">
          <div class="col-md-6">
            <h1 id="header-title">Google Trends</h1>
          </div>
        </div>
      </div>
    </header>

    <div class="container mb-5 col-md-12">
      <div id="main" class="card card-body">
        <div class="title">
          <h3>Top Trends</h3>
          <h6>Past 30 Days</h6>
          <h6>ðŸ”¥ Rising</h6>
        </div>
        <div class="row">
          <div class="col col-md-8">
            <% map.forEach(function(key, value) { %>
            <div class="container mb-4 col-md-12">
              <div id="main" class="card card-body">
                <div class="row">
                  <% if (key) { %>
                  <h5>ðŸ”¥</h5>
                  <% } %>
                  <h3><%- value %></h3>
                </div>
                <div class="row">
                  <div class="col col-md-8"></div>
                </div>
              </div>
            </div>
            <% }) %>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
